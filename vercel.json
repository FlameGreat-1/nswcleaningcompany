{
  "framework": "vite",
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    },
    {
      "source": "/robots.txt",
      "destination": "/api/robots"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/services/general-cleaning",
      "destination": "/services#general-cleaning",
      "permanent": true
    },
    {
      "source": "/services/deep-cleaning",
      "destination": "/services#deep-cleaning",
      "permanent": true
    },
    {
      "source": "/services/end-of-lease",
      "destination": "/services#end-of-lease-cleaning",
      "permanent": true
    },
    {
      "source": "/services/ndis",
      "destination": "/ndis",
      "permanent": true
    },
    {
      "source": "/quote-calculator",
      "destination": "/quote",
      "permanent": true
    },
    {
      "source": "/get-quote",
      "destination": "/quote",
      "permanent": true
    },
    {
      "source": "/contact-us",
      "destination": "/contact",
      "permanent": true
    },
    {
      "source": "/frequently-asked-questions",
      "destination": "/faq",
      "permanent": true
    }
  ]
}
